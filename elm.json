{
    "type": "package",
    "name": "mpizenberg/elm-test-runner",
    "summary": "Helper package to run tests and report results",
    "license": "BSD-3-Clause",
    "version": "5.0.0",
    "exposed-modules": [
        "ElmTestRunner.Failure",
        "ElmTestRunner.Reporter",
        "ElmTestRunner.Reporter.ConsoleColor",
        "ElmTestRunner.Reporter.ConsoleDebug",
        "ElmTestRunner.Reporter.Interface",
        "ElmTestRunner.Reporter.Json",
        "ElmTestRunner.Reporter.Exercism",
        "ElmTestRunner.Reporter.Junit",
        "ElmTestRunner.Result",
        "ElmTestRunner.Runner",
        "ElmTestRunner.SeededRunners"
    ],
    "elm-version": "0.19.1 <= v < 0.20.0",
    "dependencies": {
        "elm/core": "1.0.0 <= v < 2.0.0",
        "elm/json": "1.0.0 <= v < 2.0.0",
        "elm/random": "1.0.0 <= v < 2.0.0",
        "elm-explorations/test": "2.0.0 <= v < 3.0.0"
